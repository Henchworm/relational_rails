<%= form_with url: '/parks', method: :post, local: true do |form| %>
  <p><%= form.label :name %>
  <%= form.text_field :name %></p>

  <p><%= form.label :entrance_fee %>
  <%= form.text_field :entrance_fee %></p>

  <p><%= form.label :ocean_access %>
  <%= form.text_field :ocean_access %></p>

  <p><%=form.submit "Create Park"%></p>
<% end %>
